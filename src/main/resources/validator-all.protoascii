    value_casei: "th"
    value_casei: "tr"
    value_casei: "vi"
    value_casei: "zh-cn"
    value_casei: "zh-tw"
  }
  attrs: {
    name: "offset-seconds"
    value_regex: "-?\\d+"
  }
  attrs: {
    name: "template"
